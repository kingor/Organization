<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:DockLayoutPanel unit="EM" width="800px" height="400px">
		<g:west size="15">
			<g:DecoratorPanel>
				<g:VerticalPanel>
					<g:HorizontalPanel horizontalAlignment="ALIGN_CENTER"
						verticalAlignment="ALIGN_MIDDLE">
						<g:Button ui:field="addButton">добавить</g:Button>
						<g:Button ui:field="deleteButton">удалить</g:Button>
					</g:HorizontalPanel>
					<g:ListBox ui:field="organizationList" visibleItemCount='20' />
				</g:VerticalPanel>
			</g:DecoratorPanel>
		</g:west>
		<g:center>
			<g:VerticalPanel height="300px">
				<g:TextBox></g:TextBox>
				<g:HorizontalPanel>
					<g:Label>сведения</g:Label>
					<g:TextArea></g:TextArea>
					<g:VerticalPanel>
						<g:HorizontalPanel>
							<g:Label>адрес</g:Label>
							<g:TextBox></g:TextBox>
						</g:HorizontalPanel>
						<g:HorizontalPanel>
							<g:Label>телефон</g:Label>
							<g:TextBox></g:TextBox>
						</g:HorizontalPanel>
					</g:VerticalPanel>
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:Label>кол.сотр</g:Label>
					<g:TextBox></g:TextBox>
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:Label>доп. информация</g:Label>
					<g:TextArea></g:TextArea>
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:Label>тип</g:Label>
					<g:TextBox></g:TextBox>
					<g:Button>...</g:Button>
				</g:HorizontalPanel>
				<g:HorizontalPanel>
					<g:Label>управляющий</g:Label>
					<g:TextBox></g:TextBox>
					<g:Button>...</g:Button>
					<dp:DateBox></dp:DateBox>
				</g:HorizontalPanel>
				<g:Button>сохранить</g:Button>
			</g:VerticalPanel>
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder> 